{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "../out",
        "lib": [
            "es6"
        ],
        "rootDir": "..",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "importHelpers": true,
        "removeComments": true,
        "sourceMap": true
    }
}